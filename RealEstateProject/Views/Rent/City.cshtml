@model IEnumerable<RealEstateProject.Models.Rent>
@using RealEstateProject.Models

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>City</title>
</head>
<body style="background-color:gainsboro">
    <div style="color:darkcyan;font-size:20px">
        <h1 style="color:crimson"><u>City Names</u></h1>


        @foreach (Rent rent in Model)
            {
            @Html.ActionLink(rent.City, "Property", new { @id = rent.City })
            <br />
        }
    </div>
</body>

</html>
