@model IEnumerable<RealEstateProject.Models.Rent>
@using RealEstateProject.Models

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Property</title>
</head>
<body style="background-color:gainsboro">
    <div style="color:darkcyan;font-size:20px">
        <h1 style="color:crimson"><u>Property No under the selected City</u></h1>
        @foreach (Rent rent in Model)
        {

            @Html.ActionLink(rent.PropertyNo, "PropertyDetails", new { @id = rent.PropertyNo })
            <br />
        }
    </div>
</body>
</html>
